#!/usr/bin/python

import sys

print ("Season	Sector			Count")    
transactions = []
no_of_lines=0 
A={}
for line in sys.stdin:
    split_line = line.strip().split(',')
    if split_line[0] in A:
        A[split_line[0]]+=int(split_line[2]);	
    else :
        A[split_line[0]]=1;
#	print (split_line[0])

for k in A:
    print(k +"\t\t  "+str(A[k]))
# print (A)		

